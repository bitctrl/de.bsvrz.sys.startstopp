{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"option": {
			"type": "string",
			"enum": [
				"automatisch",
				"manuell",
				"intervallrelativ",
				"intervallabsolut"
			],
			"default": "automatisch"
		},
		"neuStart": {
			"type": "boolean",
			"default": true
		},
		"intervall": {
			"type": "string"
		}
	}}
	
}
